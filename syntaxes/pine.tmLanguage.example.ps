    b.set_text_wrap(text.wrap_none)
    int left = b.get_left()
    int right = b.get_right()
    float top = b.get_top()
    float bottom = b.get_bottom()
    b.set_left(left = bar_index - 20)
    b.set_right(right = bar_index -5)
    b.set_top(top = high + 1)
